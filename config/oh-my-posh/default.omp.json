        "diff-line": "#252535",
        "git-add": "#76946A",
        "git-delete": "#C34043",
        "git-change": "#DCA561",
        "diagnostic-error": "#E82424",
        "diagnostic-warning": "#FF9E3B",
        "diagnostic-info": "#6A9589",
        "diagnostic-hint": "#658594",
        "comments": "#727169",
        "light-foreground": "#938AA9",
