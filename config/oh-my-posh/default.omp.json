      }
    }
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "foreground": "p:dark-foreground",
          "background": "p:dark-background",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "\udb82\ude0a",
              "zsh": "\uf179"
            }
          }
        },
        {
          "type": "battery",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "p:dark-foreground",
          "background": "p:dark-background",
          "template": "{{ if not .Error }}{{ if (eq \"Discharging\" .State.String )}}->{{ .Icon }}<-{{ .Percentage }}%{{ end }}{{ end }}",
          "properties": {
            "discharging_icon": "\udb84\udea2",
            "charging_icon": "\udb84\udea5",
            "charged_icon": "\udb84\udea6"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:foreground",
          "background": "p:tangelo",
          "template": " {{ .Path }} ",
          "properties": {
            "style": "agnoster_short",
            "hide_root_location": false,
            "max_depth": 2,
            "home_icon": "\uf015",
            "folder_icon": "\uf07c",
            "folder_separator_icon": " \ue0bb "
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:foreground",
          "background": "p:git-add",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-change{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-delete{{ end }}",
            "{{ if gt .Ahead 0 }}p:maize{{ end }}",
            "{{ if gt .Behind 0 }}p:git-change{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:dark-foreground",
          "background": "p:color12",
          "template": " \ue77f {{ .Full }} "
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "p:foreground",
          "background": "p:color9",
          "template": " \uf071 "
        },
        {
          "type": "text",
          "style": "diamond",
          "foreground": "p:foreground",
          "background": "p:background",
          "background_templates": [
            "{{ if gt .Code 0 }}p:color17{{ end }}"
          ],
          "trailing_diamond": "\ue0b4",
          "template": " {{ if eq .Code 0 }}\uf164{{else}}\uf165{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ],
  "transient_prompt": {
    "foreground": "p:light-foreground",
    "foreground_templates": [
      "{{ if gt .Code 0 }}p:color17{{ end }}"
    ],
    "background": "transparent",
    "template": "{{ .Shell }} \uf178 "
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "p:light-foreground",
    "template": "\uf061 "
  }
}
